{
  "version": 2,
  "buildCommand": "cd app && npm run build",
  "outputDirectory": "app/dist",
  "framework": null,
  "installCommand": "npm install",
  "devCommand": "cd app && npm run dev",
  "rewrites": [
    {
      "source": "/data/(.*)",
      "destination": "/data/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "/api/$1.json"
    },
    {
      "source": "/health",
      "destination": "/health.json"
    },
    {
      "source": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$",
      "destination": "/$1"
    },
    {
      "source": "/((?!api|data|health|assets).*)",
      "destination": "/index.html"
    }
  ],
  "headers": [
    {
      "source": "/data/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "x-vercel-protection-bypass",
          "value": "true"
        }
      ]
    }
  ]
}
